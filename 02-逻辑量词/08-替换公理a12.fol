// aea.iff 

thm nf.a4i.aea.iff.1(set s0, prop p0, prop p1) {
  |- iff(p0, forall(s0,p1))
  |- iff(forall(s0,p1), p0)
  |- imp(p0, forall(s0,p1))
  |- imp(forall(s0,p1), p0)
  |- iff(p0, exist(s0,p1))
  |- iff(exist(s0,p1), p0)
  |- imp(p0, exist(s0,p1))
  |- imp(exist(s0,p1), p0)
  -| forall(s0, iff(p0,p1))
  -| nf(s0, p0)
} = {
  mp(iff(p0,forall(s0,p1)), forall(s0,iff(p0,p1)))
  mp(iff(forall(s0,p1),p0), forall(s0,iff(p0,p1)))
  mp(imp(p0,forall(s0,p1)), forall(s0,iff(p0,p1)))
  mp(imp(forall(s0,p1),p0), forall(s0,iff(p0,p1)))
  mp(iff(p0,exist(s0,p1)), forall(s0,iff(p0,p1)))
  mp(iff(exist(s0,p1),p0), forall(s0,iff(p0,p1)))
  mp(imp(p0,exist(s0,p1)), forall(s0,iff(p0,p1)))
  mp(imp(exist(s0,p1),p0), forall(s0,iff(p0,p1)))
  nf.a4.aea.iff.diff(s0, p0, p1)
}

thm nf.a4id.aea.iff.1(set s0, prop p0, prop p1, prop p2) {
  |- imp(p2, iff(p0, forall(s0,p1)))
  |- imp(p2, iff(forall(s0,p1), p0))
