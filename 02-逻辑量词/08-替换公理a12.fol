  mpd(p2, iff(p0,forall(s0,p1)), forall(s0,iff(p0,p1)))
  syl(p2, imp(forall(s0,iff(p0,p1)),iff(p0,forall(s0,p1))), nf(s0,p0))
  mpd(p2, iff(forall(s0,p1),p0), forall(s0,iff(p0,p1)))
  syl(p2, imp(forall(s0,iff(p0,p1)),iff(forall(s0,p1),p0)), nf(s0,p0))
  mpd(p2, imp(p0,forall(s0,p1)), forall(s0,iff(p0,p1)))
  syl(p2, imp(forall(s0,iff(p0,p1)),imp(p0,forall(s0,p1))), nf(s0,p0))
  mpd(p2, imp(forall(s0,p1),p0), forall(s0,iff(p0,p1)))
  syl(p2, imp(forall(s0,iff(p0,p1)),imp(forall(s0,p1),p0)), nf(s0,p0))
  mpd(p2, iff(p0,exist(s0,p1)), forall(s0,iff(p0,p1)))
  syl(p2, imp(forall(s0,iff(p0,p1)),iff(p0,exist(s0,p1))), nf(s0,p0))
  mpd(p2, iff(exist(s0,p1),p0), forall(s0,iff(p0,p1)))
  syl(p2, imp(forall(s0,iff(p0,p1)),iff(exist(s0,p1),p0)), nf(s0,p0))
  mpd(p2, imp(p0,exist(s0,p1)), forall(s0,iff(p0,p1)))
  syl(p2, imp(forall(s0,iff(p0,p1)),imp(p0,exist(s0,p1))), nf(s0,p0))
  mpd(p2, imp(exist(s0,p1),p0), forall(s0,iff(p0,p1)))
  syl(p2, imp(forall(s0,iff(p0,p1)),imp(exist(s0,p1),p0)), nf(s0,p0))
  nf.a4.origin.aea.iff.diff.1(s0, p0, p1)
